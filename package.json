{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config config/webpack.run.js --progress --env.development",
    "build:client": "webpack --config config/webpack.run.js --progress --env.production",
    "build:server": "webpack --config config/webpack.run.js --progress --env.server",
    "build": "npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node dist/server.*.bundle.js",
    "debug-ssr": "node --inspect dist/server.*.bundle.js",
    "lint": "eslint . --ext .js,.jsx"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-hot-loader": "^4.3.7",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "styled-jsx": "^3.0.2",
    "wicg-focus-ring": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "styled-jsx-plugin-sass": "^0.2.4",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}

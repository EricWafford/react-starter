{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config config/webpack.run.js --progress --env.development",
    "build:client": "webpack --config config/webpack.run.js --progress --env.production",
    "build:server": "webpack --config config/webpack.run.js --progress --env.server",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/server.*.bundle.js",
    "debug-ssr": "node --inspect dist/server.*.bundle.js",
    "lint": "eslint src/ --ext .js --ext .jsx"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^4.5.0",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "styled-jsx": "^2.2.5",
    "wicg-focus-ring": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "git-revision-webpack-plugin": "^2.5.1",
    "html-webpack-plugin": "^3.0.4",
    "node-sass": "^4.7.2",
    "styled-jsx-plugin-sass": "^0.2.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  }
}
